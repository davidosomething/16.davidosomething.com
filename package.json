{
  "name": "16-davidosomething-com",
  "homepage": "http://davidosomething.com",
  "version": "1.0.0",
  "description": "2016 theme for davidosomething.com",
  "license": "UNLICENSED",
  "author": {
    "name": "David O'Trakoun",
    "email": "me@davidosomething.com",
    "url": "http://davidosomething.com"
  },
  "contributors": [
    "David O'Trakoun <me@davidosomething.com> (http://davidosomething.com/)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davidosomething.com/16.davidosomething.com.git"
  },
  "bugs": {
    "url": "https://github.com/davidosomething/16.davidosomething.com/issues",
    "email": "me@davidosomething.com"
  },
  "keywords": [],
  "private": true,
  "engines": {
    "node": ">=4.2.0"
  },
  "scripts": {
    "js": "jspm bundle-sfx assets/js/global public/assets/js/global.js"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "arrow-body-style": [
        2,
        "always"
      ],
      "arrow-parens": [
        2,
        "always"
      ],
      "arrow-spacing": [
        2,
        {
          "before": true,
          "after": true
        }
      ],
      "brace-style": [
        1,
        "stroustrup"
      ],
      "block-scoped-var": 2,
      "block-spacing": 2,
      "camelcase": 1,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "comma-style": [
        2,
        "last"
      ],
      "computed-property-spacing": [
        2,
        "never"
      ],
      "eol-last": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "max-nested-callbacks": [
        1,
        3
      ],
      "max-params": [
        2,
        3
      ],
      "max-depth": [
        2,
        3
      ],
      "new-cap": 2,
      "new-parens": 2,
      "no-cond-assign": 2,
      "no-console": 1,
      "no-constant-condition": 2,
      "no-control-regex": 2,
      "no-debugger": 1,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty-character-class": 2,
      "no-empty": 2,
      "no-ex-assign": 2,
      "no-extra-boolean-cast": 2,
      "no-extra-parens": 2,
      "no-extra-semi": 2,
      "no-func-assign": 2,
      "no-inner-declarations": 2,
      "no-invalid-regexp": 2,
      "no-irregular-whitespace": 2,
      "no-mixed-spaces-and-tabs": [
        2,
        "smart-tabs"
      ],
      "no-negated-in-lhs": 2,
      "no-obj-calls": 2,
      "no-regex-spaces": 2,
      "no-spaced-func": 2,
      "no-sparse-arrays": 2,
      "no-unexpected-multiline": 2,
      "no-unreachable": 2,
      "no-use-before-define": 2,
      "use-isnan": 2,
      "no-floating-decimal": 1,
      "no-magic-numbers": [
        1,
        {
          "ignore": [
            -1,
            0,
            1,
            2
          ]
        }
      ],
      "no-nested-ternary": 2,
      "no-plusplus": 1,
      "no-throw-literal": 2,
      "no-trailing-spaces": 2,
      "no-underscore-dangle": 0,
      "object-curly-spacing": [
        2,
        "always"
      ],
      "one-var": [
        2,
        "never"
      ],
      "quote-props": [
        2,
        "as-needed",
        {
          "keywords": true
        }
      ],
      "quotes": [
        1,
        "single"
      ],
      "radix": 2,
      "semi": [
        2,
        "always"
      ],
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "space-after-keywords": 2,
      "space-before-blocks": 2,
      "space-before-function-paren": [
        1,
        {
          "anonymous": "always",
          "named": "never"
        }
      ],
      "space-before-keywords": [
        2,
        "always"
      ],
      "space-infix-ops": [
        2,
        {
          "int32Hint": false
        }
      ],
      "space-return-throw-case": 2,
      "space-unary-ops": 2,
      "valid-jsdoc": [
        1,
        {
          "requireParamDescription": false,
          "requireReturn": false,
          "requireReturnDescription": false
        }
      ],
      "valid-typeof": 2,
      "wrap-iife": [
        2,
        "any"
      ],
      "yoda": [
        2,
        "never"
      ]
    }
  },
  "jspm": {
    "directories": {
      "packages": "assets/jspm"
    },
    "dependencies": {
      "highlight.js": "npm:highlight.js@^8.9.1",
      "jquery": "github:components/jquery@^2.1.4",
      "normalize.css": "github:necolas/normalize.css@^3.0.3"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-core": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "cssnano": "^3.3.2",
    "debug": "^2.2.0",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^1.1.1",
    "gulp-postcss": "^6.0.1",
    "gulp-sass": "^2.1.0",
    "gulp-sass-lint": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-through": "^0.4.0",
    "gulpsmith": "^0.5.5",
    "handlebars": "^4.0.5",
    "helper-moment": "^0.1.0",
    "jspm": "^0.16.18",
    "lodash.omit": "^3.1.0",
    "merge-stream": "^1.0.0",
    "metalsmith": "^2.1.0",
    "metalsmith-branch": "0.0.4",
    "metalsmith-build-date": "^0.2.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-define": "^1.0.0",
    "metalsmith-headings": "^0.1.0",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-layouts": "^1.4.2",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-matters": "^1.1.0",
    "metalsmith-paths": "^2.1.1",
    "metalsmith-permalinks": "^0.4.0",
    "metalsmith-snippet": "^2.0.0",
    "metalsmith-widow": "^1.1.1",
    "slug": "^0.9.1"
  }
}
